     "shell.execute_reply": "2025-04-01T20:27:01.939577Z"
    }
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "6857\n"
     ]
    }
   ],
   "source": [
    "n = 600851475143\n",
    "p = 2\n",
    "while n != 1:\n",
    "    while n % p != 0:\n",
    "        p += 1\n",
    "    n //= p\n",
    "print(p)"
   ]
  }
